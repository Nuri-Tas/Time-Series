  {
   "cell_type": "code",
   "execution_count": 210,
   "id": "95aaa291",
   "metadata": {},
   "outputs": [],
   "source": [
    "def retvrn(c):\n",
    "    return c* 9 / 5 + 32 \n",
    "\n",
    "df['AvgTemperature'] = df['AvgTemperature'].apply(lambda x: retvrn(x))"
   ]
